{
    "name": "CollectedNotes private default",
    "description": "Sets the visibility of new notes to private",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "matches": ["https://collectednotes.com/*/notes/new"],
          "run_at": "document_idle",
          "js": ["content-script.js"]
        }
    ]
}